def evaluate_score():
    try:
        score = int(input("请输入一个分数（0-100）："))
        if score < 0 or score > 100:
            print("分数必须在0到100之间！")
        elif score >= 90:
            print("优秀")
        elif score >= 75:
            print("良好")
        elif score >= 60:
            print("及格")
        else:
            print("不及格")
    except ValueError:
        print("请输入一个有效的数字！")

if __name__ == "__main__":
    evaluate_score()
